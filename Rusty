-- Load everything in the Workspace, including nested folders
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Function to access all objects in the Workspace
local function loadAllObjects()
    local objectsLoaded = 0

    -- Iterate through all descendants of the Workspace
    for _, object in ipairs(workspace:GetDescendants()) do
        if object:IsA("BasePart") or object:IsA("Model") then
            -- Simply accessing the object to ensure it's loaded
            local name = object.Name -- Accessing the name to ensure it's loaded
            objectsLoaded = objectsLoaded + 1
        end
    end

    print(objectsLoaded .. " objects accessed.")
end

-- Call the function to access all objects
loadAllObjects()
